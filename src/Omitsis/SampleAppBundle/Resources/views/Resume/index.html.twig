<h1>Resume</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Owner</th>
            <th>City</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
    {% for result in results %}
        <tr>
			<td>{{result.owner}}</td>
            <td>{{result.city}}</td>
			<td>{{result.name}}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>


<h1>Resume group whith count</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Owner</th>
            <th>City</th>
            <th>Number of boats</th>
        </tr>
    </thead>
    <tbody>
    {% for resultg in resultsGroup %}
        <tr>
			<td>{{resultg.owner}}</td>
            <td>{{resultg.city}}</td>
			<td>{{resultg.n}}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>

